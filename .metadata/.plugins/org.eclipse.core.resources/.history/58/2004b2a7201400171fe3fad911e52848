package com.cn.test1;

public class Test {

	public static void main(String[] args) {
		//对象间的赋值属于引用传递
		Dog mydog = new Dog();
		mydog.color="yellow";
		mydog.name="小黄";
		mydog.sex="man";
		
		Dog yourdog=new Dog();
		yourdog.name="小黑";
		yourdog.color="red";
		yourdog.sex="woman";
		
		yourdog=mydog;
		System.out.println(yourdog.name);
		
		
//		Dog harry = new Dog();
